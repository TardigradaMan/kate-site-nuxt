{
  "extends": "stylelint-config-standard",
  "rules": {
    "value-list-comma-newline-after": null,
    "declaration-colon-newline-after": null,
    "at-rule-no-unknown": [
      true,
      {
        "ignoreAtRules": [
          "function",
          "if",
          "each",
          "include",
          "mixin",
          "for"
        ]
      }
    ]
  }
}
